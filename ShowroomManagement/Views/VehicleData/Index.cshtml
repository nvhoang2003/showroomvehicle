@model IEnumerable<ShowroomManagement.Models.vehicle_data>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.color)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.listId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.vehicle.name)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.color)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.listId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.vehicle.name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.vehicle_data_id }) |
                @Html.ActionLink("Details", "Details", new { id=item.vehicle_data_id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.vehicle_data_id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
